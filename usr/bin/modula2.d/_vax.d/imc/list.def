definition module list;
from system import Address;
export qualified List, AddToList, Next;

type
    List = pointer to ListRec;
    ListRec = record
	first, last : Address;
    end;
procedure AddToList(list : List; newOne : Address) : List;
procedure Next(element : Address) : Address;
end list.
